{
	"$schema": "./node_modules/@angular/service-worker/config/schema.json",
	"index": "/index.html",
	"assetGroups": [
		{
			"name": "app",
			"installMode": "prefetch",
			"resources": {
				"files": [
					"/favicon.ico",
					"/index.html",
					"/manifest.webmanifest",
					"/*.css",
					"/*.js",
					"/assets/fontawesome/**"
				],
				"urls": [
					"**.typekit.net**"
				]
			}
		},
		{
			"name": "pieces",
			"installMode": "prefetch",
			"updateMode": "prefetch",
			"resources": {
				"files": [
					"/assets/images/pieces/**"
				]
			}
		},
		{
			"name": "config",
			"installMode": "prefetch",
			"updateMode": "prefetch",
			"resources": {
				"files": [
					"/assets/config/**"
				]
			}
		},
		{
			"name": "assets",
			"installMode": "lazy",
			"updateMode": "prefetch",
			"resources": {
				"files": [
					"/assets/**",
					"/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
				]
			}
		}
	]
}